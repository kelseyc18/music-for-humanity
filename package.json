{
  "name": "music-for-humanity",
  "version": "0.0.0",
  "engines": {
    "node": "v8.9.0"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev-start": "nodemon ./bin/www",
    "build-js": "browserify public/javascripts/player.js > public/javascripts/player_bundle.js; browserify public/javascripts/judge.js > public/javascripts/judge_bundle.js;",
    "watch-player-js": "watchify public/javascripts/player.js -o public/javascripts/player_bundle.js -v",
    "watch-judge-js": "watchify public/javascripts/judge.js -o public/javascripts/judge_bundle.js -v"
  },
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "~1.16.0",
    "bopper": "^2.11.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "ditty": "^2.6.0",
    "express": "~4.14.1",
    "express-validator": "^4.3.0",
    "jade": "~1.11.0",
    "mongodb": "^2.2.25",
    "mongoose": "^4.13.2",
    "monk": "^4.0.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "watchify": "latest"
  }
}
